## -*- shell-script -*-

TOOL=dconv
CMDLINE="-qi \"AM %d.%m.%Y\""

## STDIN
cat > "${TS_STDIN}" <<EOF
 FOLGENDE WERTPAPIERE WERDEN AM 02.10.2013 EX DIVIDENDE GEHANDELT.
 THE EX-DIVIDEND DAY OF THE FOLLOWING SHARES WILL BE 02.10.2013.

 INSTR EXCH ISIN INSTRUMENT NAME REF.PRC ADJUST
EOF
 
## STDOUT
cat > "${TS_EXP_STDOUT}" <<EOF
2013-10-02
EOF

