## -*- shell-script -*-

TOOL=dconv
CMDLINE="--from-zone America/Chicago"
COMMENT="Testing illegal datetimes, 02:05:06 doesn't exist on 11 Mar 2012 in \
America/Chicago"

## STDIN
cat > "${TS_STDIN}" <<EOF
2012-03-11T02:05:06
EOF
 
## STDOUT
cat > "${TS_EXP_STDOUT}" <<EOF
2012-03-11T07:05:06
EOF

