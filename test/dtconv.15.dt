## -*- shell-script -*-

TOOL=dconv
CMDLINE="--zone America/Chicago -S -i '%F %H:%M' -f '%T'"

## STDIN
cat > "${TS_STDIN}" <<EOF
now	2012-03-12 09:05
+1m	2012-03-12 09:06	later than last line
EOF
 
## STDOUT
cat > "${TS_EXP_STDOUT}" <<EOF
now	04:05:00
+1m	04:06:00	later than last line
EOF

