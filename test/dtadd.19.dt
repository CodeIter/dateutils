## -*- shell-script -*-

TOOL=dadd
CMDLINE="'2012-01-02 02:04:00' -f '%F %T'"
COMMENT="mass duration adder"

## STDIN
cat > "${TS_STDIN}" <<EOF
-40h
-1d2h4m
-1w2d6h3m
-1mo 6m
hello
EOF

## STDOUT
cat > "${TS_EXP_STDOUT}" <<EOF
2011-12-31 10:04:00
2012-01-01 00:00:00
2011-12-23 20:01:00
2011-12-02 01:58:00
EOF

## STDERR
cat > "${TS_EXP_STDERR}" <<EOF
dadd: cannot make sense of \`hello' using the given input formats
EOF

## dtadd.19.dt ends here
